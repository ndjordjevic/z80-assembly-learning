{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "make (sjasmplus) - 8-bit Load",
            "type": "shell",
            "command": "mkdir -p build && sjasmplus --raw=${workspaceFolder}/build/8bit-load.obj --sld=${workspaceFolder}/build/8bit-load.sld --fullpath ${workspaceFolder}/8bit-load.asm",
            "problemMatcher": {
                "owner": "sjasmplus",
                "fileLocation": "autoDetect",
                "pattern": {
                    "regexp": "^(.*)\\((\\d+)\\):\\s+(warning|error):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "severity": 3,
                    "message": 4
                }
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
